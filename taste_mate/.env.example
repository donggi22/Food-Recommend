# LLM 호출용 (없으면 fallback 응답 사용)
# 이 값을 복사해서 .env 파일을 만들고, 그 안에 넣으세요. .env는 git에 올리지 마세요.
# OPENAI_API_KEY=sk-your-key-here
# OPENAI_BASE_URL=https://your-endpoint.openai.azure.com/
# LLM_MODEL=gpt-4o-mini
# LLM_TEMPERATURE=0.3  (재현성 검증 시 0 권장)


# Vertex AI 설정
GOOGLE_CLOUD_PROJECT="내-프로젝트-ID" # 내프로젝트 ID로 수정(API json파일 명이랑 동일)
GOOGLE_CLOUD_LOCATION="us-central1"

# 서비스 계정 키 파일의 전체 경로 (예시)
GOOGLE_APPLICATION_CREDENTIALS="C:/절대경로/service-account-key.json" # GCP에서 받은 API 경로로 수정

# 모델 설정
LLM_MODEL="gemini-2.0-flash"
LLM_TEMPERATURE="0.3"